${
	string Verb(Method m)
	{
		return m.Attributes.FirstOrDefault(a => a.Name.Contains("Http")).Name.Replace("Http", "").ToLower();
	}
}
module App {$Classes(*WebApiController)[    
    export class $Name$IsGeneric[<T>][] {
		$Methods[
		public $name = ($Parameters[$name: $Type][, ]) : Promise<$Type[$GenericTypeArguments[$GenericTypeArguments[$Type]]]> => {
            var verb = '$Verb';
            var route = '$Route';
        };]
    }]
}